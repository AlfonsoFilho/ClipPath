{
  "name": "clip-path",
  "version": "1.1.0",
  "keywords": [
    "jquery",
    "polyfill",
    "clip-path"
  ],
  "description": "ClipPath",
  "main": "./dist/clippath.min.js",
  "scripts": {
    "test": "standard --fix \"src/**/*.js\" | snazzy && jest",
    "test:watch": "jest --watch",
    "build:babel": "babel src -d dist --ignore \"src/**/*.spec.js\"",
    "build:uglify": "uglifyjs ./dist/clippath.js -o ./dist/clippath.min.js --compress --mangle --source-map",
    "build": "run-s build:*",
    "start": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfonsoFilho/ClipPath.git"
  },
  "author": "Alfonso Filho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfonsoFilho/ClipPath/issues"
  },
  "homepage": "https://github.com/AlfonsoFilho/ClipPath#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "babel-jest": "24.1.0",
    "jest": "24.1.0",
    "npm-run-all": "4.1.5",
    "snazzy": "8.0.0",
    "standard": "12.0.1",
    "uglify-js": "3.4.9"
  },
  "engines": {
    "node": ">=8"
  }
}